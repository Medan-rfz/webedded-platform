{
    "version": "2",
	"sql": [
		{
			"engine": "postgresql",
			"schema": "../migrations",
			"gen": {
				"go": {
					"package": "users",
					"out": "../internal/infrastructure/repositories/users/queries",
					"sql_package": "pgx/v5",
					"emit_json_tags": true,
					"emit_interface": true,
					"omit_unused_structs": true
				}
			},
			"queries": "../internal/infrastructure/repositories/users/queries/queries.sql"
		}
    ]
}